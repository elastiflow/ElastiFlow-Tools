# Unattended installation
FULL_AUTO=1

# Password for the 'elastic' user (at least 6 characters)
ELASTIC_PASSWORD=elastic

# Password for the 'kibana_system' user (at least 6 characters)
KIBANA_PASSWORD=elastic

# Version of Elastic products
STACK_VERSION=8.15.2

# Set the cluster name
CLUSTER_NAME=docker-cluster

# Set to 'basic' or 'trial' to automatically start the 30-day trial
LICENSE=basic
#LICENSE=trial

# Port to expose Elasticsearch HTTP API to the host
ES_PORT=9200
#ES_PORT=127.0.0.1:9200

# Port to expose Kibana to the host
KIBANA_PORT=5601
#KIBANA_PORT=80

# JVM Heap Size
# Set heap size to about one-third of the system memory, but do not exceed 31g. Assuming 16GB of system memory so set to 5GB
JVM_HEAP_SIZE=5

# Set the memory limit to 2x the heap size (set to 10GB)
MEM_LIMIT_ELASTIC=10737418240

# Set the memory limit to 2GB for small to medium workloads (set to 2GB)
MEM_LIMIT_KIBANA=2147483648

# Project namespace (defaults to the current folder name if not set)
#COMPOSE_PROJECT_NAME=myproject

# ElastiFlow Flow Version
ELASTIFLOW_FLOW_VERSION=7.5.0
# Not required to install and use but does unlock additional features
ELASTIFLOW_FLOW_ACCOUNT_ID=
ELASTIFLOW_FLOW_KEY=

# ElastiFlow SNMP Version
ELASTIFLOW_SNMP_VERSION=7.5.0
# Required to install and use
ELASTIFLOW_SNMP_ACCOUNT_ID=
ELASTIFLOW_SNMP_KEY=

DASHBOARDS_VERSION=8.14.x
DASHBOARDS_CODEX_ECS=codex

# Elastic X-Pack Saved Objects Encryption Key

